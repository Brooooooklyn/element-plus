{
  "name": "element-plus",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "cz": "npx git-cz",
    "test": "jest",
    "gen": "bash ./scripts/gc.sh",
    "storybook": "start-storybook",
    "bootstrap": "yarn && npx lerna bootstrap",
    "build": "yarn bootstrap && yarn build-storybook",
    "lint": "eslint ./packages --ext .vue,.js,.ts",
    "website-build": "webpack --config ./website/webpack.config.js",
    "website-dev": "webpack-dev-server --config ./website/webpack.config.js",
    "website-dev:play": "PLAY_ENV=true yarn website-dev"
  },
  "peerDependencies": {
    "vue": "^3.0.0-rc.9"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.1",
    "@storybook/addon-storysource": "^6.1.20",
    "@storybook/html": "^6.1.20",
    "@swc-node/jest": "^1.1.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.10.1",
    "@vue/compiler-sfc": "^3.0.6",
    "@vue/component-compiler-utils": "^3.2.0",
    "algoliasearch": "^4.8.5",
    "babel-loader": "^8.2.2",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.20.0",
    "eslint-plugin-vue": "^7.0.0-beta.0",
    "file-loader": "^6.2.0",
    "highlight.js": "^10.6.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lerna": "^4.0.0",
    "lint-staged": "^10.5.4",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.2",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^3.0.0",
    "sass": "^1.32.8",
    "sass-loader": "^10.0.1",
    "style-loader": "^2.0.0",
    "throttle-debounce": "3.0.1",
    "transliteration": "^2.2.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.2",
    "url-loader": "^4.1.1",
    "vite": "^1.0.0-rc.4",
    "vue-jest": "5.0.0-alpha.1",
    "vue-loader": "16.1.2",
    "vue-router": "^4.0.4",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push-todo-open": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix"
  },
  "workspaces": [
    "packages/*"
  ],
  "description": "A Component Library for Vue3.0",
  "main": "index.js",
  "repository": "git@github.com:element-plus/element-plus.git",
  "license": "MIT",
  "dependencies": {
    "@popperjs/core": "^2.8.6",
    "dayjs": "1.x",
    "lodash": "^4.17.21",
    "mitt": "^2.1.0"
  }
}
